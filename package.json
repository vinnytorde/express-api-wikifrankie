{
  "name": "express-wikifrankie-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev:api": "nodemon ./bin/www",
    "dev": "concurrently \"yarn:dev:api\" \"cd client && yarn start\" -n api,client",
    "heroku-postbuild": "cd client && yarn && yarn build"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.3",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "body-parser": "^1.18.3",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^7.0.0",
    "express": "~4.16.0",
    "express-session": "^1.15.6",
    "morgan": "~1.9.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.9.0",
    "sequelize": "^5.3.5"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "nodemon": "^1.18.11",
    "prettier": "^1.17.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,scss,md}": [
      "prettier --config .prettierconfig.yml --write",
      "git add"
    ]
  }
}
